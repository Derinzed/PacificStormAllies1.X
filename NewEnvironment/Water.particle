Water/SmallTrack
{
      view_distance   6000
      material          Water/Track
      particle_width    10
      particle_height   10
      cull_each         false
      quota             20
      billboard_type    oriented_strong
      y_axis            0 0 1
      x_axis           -1 0 0 
        
        emitter Ellipsoid
    	{
		position    		0 1 0        	
		width 			10
		height 			0
		depth			10
		duration		0.1 	
        	emission_rate       	500 
        	time_to_live_min       	1.5	
        	time_to_live_max       	1.8	
		angle               	0
    		direction       	1 0 1
        	velocity        	0
		colour			0  0  0  0
	}
     
    affector Rotator
    {
          rotation_range_start         0
          rotation_range_end           320
          rotation_speed_range_start  -500
          rotation_speed_range_end     500
    }

   
	affector ColourInterpolator 
	{ 
   		time0      0 
		colour0    1  1  1 0  
   
   		time1      0.01 
   		colour1    1  1  1 0.5 
   
   		time2      0.9
   		colour2    1  1  1  1 
   		
                time3      1 
   		colour3    1  1  1  0
  
                time4      1 
   		colour3    1  1  1  0
    	} 
	

	affector Frictioner
	{
		angular 5
	}
	
		  
}




