Water/Wave
{
	view_distance   6000
    material          Water/Wave

    particle_width    50
    particle_height   150
    cull_each         false
    quota             100
    billboard_type    oriented_strong   
    y_axis            0 0 1
    x_axis           -1 0 0     

    emitter Box 
    {
		duration_min        10
		duration_max        100
		colour				0 0 0 0.9
		width  				100
		height 				5
		depth  				100
		position 			0 1 0
		angle				180
		direction			0 0 0

		emission_rate		4
		time_to_live_min	1
		time_to_live_max	4
		velocity			2
    }

	affector Scaler
	{
		rate 10
	}	

	affector ColourInterpolator 
	{ 
		time0      0 
		colour0    0 0 0  

		time1      0.5 
		colour1    0.5 0.5 0.5  

		time2      0.9 
		colour2    0.4  0.4  0.4 

		time3      1 
		colour3    0  0  0 

		time4      1 
		colour3    0  0  0  
  	}     
}


