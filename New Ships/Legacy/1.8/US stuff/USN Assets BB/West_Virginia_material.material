material Virginia_ladders
{
    technique
    {
        pass
        {
            ambient 1.0 1.0 1.0

            cull_hardware none
            cull_software none

            texture_unit
            {
                texture Virginia_ladders.dds
    filtering trilinear

                alpha_rejection greater_equal 128
            }
        }
    }
}
